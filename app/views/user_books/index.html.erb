<h1> Reading Log</h1>

<h3>Add a new book</h3>
<%= form_for @book do |form| %> 
  <div>
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>
  <div>
    <%= form.label :author %>
    <%= form.text_field :author %>
  </div>
  <div>
    <%= form.submit "Add new book" %>
  </div>
<% end %>




